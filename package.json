{
    "name": "whs",
    "main": "node_modules/expo/AppEntry.js",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "doctor": "expo doctor",
        "eject": "expo eject",
        "test": "npx jest",
        "lint": "tslint --project ./tsconfig.json",
        "compile": "tsc",
        "todo": "git grep --heading -EI \"TODO|FIXME\" -- :^.*",
        "lint:ci": "npm run lint -- -t junit -o reports/junit/tslint-results.xml",
        "test:ci": "npx jest --ci --reporters=default --reporters=jest-junit --runInBand"
    },
    "dependencies": {
        "@babel/core": "^7.2.0",
        "expo": "^31.0.2",
        "fbemitter": "^2.1.1",
        "fetch-mock": "^7.2.5",
        "ical.js": "^1.3.0",
        "mobx": "^5.6.0",
        "mobx-persist": "^0.4.1",
        "mobx-react": "^5.4.3",
        "moment": "^2.22.2",
        "react": "16.5.0",
        "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
        "react-native-picker-select": "^5.1.0",
        "react-native-tableview-simple": "^1.0.0",
        "react-native-vector-icons": "^6.0.2",
        "react-navigation": "^2.18.2"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.1.0",
        "@types/expo": "^30.0.0",
        "@types/fetch-mock": "^6.0.4",
        "@types/jest": "^23.3.10",
        "@types/node": "^10.12.14",
        "@types/react": "^16.7.13",
        "@types/react-dom": "^16.0.9",
        "@types/react-native": "^0.57.17",
        "@types/react-native-vector-icons": "^4.6.4",
        "@types/react-navigation": "^2.13.1",
        "babel-jest": "^23.6.0",
        "babel-preset-expo": "^5.0.0",
        "expo-cli": "^2.6.12",
        "jest": "^23.6.0",
        "jest-junit": "^5.2.0",
        "ts-jest": "^23.10.5",
        "tslint": "^5.11.0",
        "tslint-react": "^3.6.0",
        "typescript": "^3.2.1"
    },
    "license": "GPL-3.0",
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
            "\\.(ts|tsx)$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "testPathIgnorePatterns": [
            "\\.snap$",
            "<rootDir>/node_modules/"
        ],
        "cacheDirectory": ".jest/cache",
        "automock": false
    }
}
