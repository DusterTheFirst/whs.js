{
  "expo": {
    "android": {
      "package": "com.dusterthefirst.whs"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "description": "Helper app for Wellesley High School",
    "githubUrl": "https://github.com/DusterTheFirst/whs.js/",
    "icon": "./assets/icon.png",
    "ios": {
      "bundleIdentifier": "com.dusterthefirst.whs",
      "supportsTablet": true
    },
    "name": "WHS",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android"
    ],
    "primaryColor": "#ffffff",
    "privacy": "public",
    "scheme": "whsjs",
    "sdkVersion": "35.0.0",
    "slug": "WHS",
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "version": "0.0.1",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "dusterthefirst",
            "project": "whs"
          }
        }
      ]
    }
  }
}
