{
    "expo": {
        "name": "WHS",
        "description": "Helper app for Wellesley High School",
        "slug": "WHS",
        "privacy": "public",
        "sdkVersion": "31.0.0",
        "platforms": [
            "ios",
            "android"
        ],
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "githubUrl": "https://github.com/DusterTheFirst/whs.js/",
        "scheme": "whs",
        "entryPoint": "src/App"
    }
}
